{
  "version": 2,
  "builds": [
    {
      "src": "api/submit.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/submit",
      "dest": "/api/submit.js"
    },
    {
      "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot|webp|vcf))",
      "dest": "/public/$1"
    },
    {
      "src": "/charity-form.html",
      "dest": "/public/charity-form.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
